{
  "manifest_version": 3,
  "name": "MCGA",
  "version": "3.0",
  "description": "MCGA Chrome Extension for computing primes",
  "action": {
    "default_popup": "Popup.html",
    "default_icon": "2620477.png"
  },
  "icons": {
    "16": "2620477.png",
    "48": "2620477.png",
    "128": "2620477.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}